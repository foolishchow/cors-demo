{
  "compilerOptions": {
    "noEmit": true,
    "target": "es2016",
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "module": "es2015",
    "moduleResolution": "node",
    "noImplicitAny": true
  }
}